<%- model_class = SupplementSubCategory -%>
<div class="page-header">
  <h1><%=t '.title', :default => @supplement_sub_category.name.pluralize %></h1>
</div>

<%= render 'users/supplements' %>


<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              supplement_sub_categories_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_supplement_sub_category_path(@supplement_sub_category), :class => 'btn btn-primary' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              supplement_sub_category_path(@supplement_sub_category),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
