<%- model_class = SupplementCategory -%>
<div class="page-header">
  <h2><%=t '.title', :default => @supplement_category.name.pluralize %></h2>
</div>

<%= render 'users/supplements' %>

<div class="form-actions">
  <%= link_to t('.back', :default => t("helpers.links.back")),
              supplement_categories_path, :class => 'btn'  %>
  <%= link_to t('.edit', :default => t("helpers.links.edit")),
              edit_supplement_category_path(@supplement_category), :class => 'btn btn-primary' %>
  <%= link_to t('.destroy', :default => t("helpers.links.destroy")),
              supplement_category_path(@supplement_category),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Are you sure?')) },
              :class => 'btn btn-danger' %>
</div>
